    "node-pg-migrate": "^6.2.2",
    "pg": "^8.8.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node": "^18.7.14",
    "@types/pg": "^8.6.5",
    "concurrently": "^7.4.0",
    "nodemon": "^2.0.19",
    "ts-node": "^10.9.1",
